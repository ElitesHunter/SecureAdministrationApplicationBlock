@model EnterpriseServices.SecurityService.OpenAPIs.Models.ApiResultBase
@{
    Layout = null;
    this.Response.ContentType = "text/xml";
}
<?xml version="1.0" encoding="utf-8"?>
<eh:enterpriseservices xmlns:eh="urn:Masterduner@@Yeah.net">
    <eh:securityservice>
        <eh:openapis>
            <eh:expando>
                <eh:signature url="@this.Request.Url.AbsoluteUri" />
                <eh:apiversion value="@Model.Version" />
            </eh:expando>
            <eh:api type="@Model.ResultType.AssemblyQualifiedName">
                <eh:state successful="@Model.Successful" code="@Model.StateCode" />
                <eh:errorcode value="@Model.ErrorCode" />
                <eh:returncontent>
                    @RenderBody()
                </eh:returncontent>
            </eh:api>
        </eh:openapis>
    </eh:securityservice>
</eh:enterpriseservices>